{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@config/(.*)": "<rootDir>/../src/config/$1",
    "@crawler/(.*)": "<rootDir>/../src/crawler/$1",
    "@schemas/(.*)": "<rootDir>/../src/schemas/$1"
  }
}
